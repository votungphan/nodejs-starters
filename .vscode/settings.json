{
   "liveServer.settings.multiRootWorkspaceName": "gas_apps_webpack_starter",
   "editor.formatOnSave": true,
   "editor.defaultFormatter": "esbenp.prettier-vscode",
   "editor.formatOnPaste": true,
   "files.trimFinalNewlines": true,
   "workbench.editor.highlightModifiedTabs": true,
   "editor.fontSize": 13,
   "editor.tabSize": 3,
   "editor.cursorBlinking": "smooth",
   "editor.minimap.enabled": false,
   "explorer.sortOrder": "type",
   "cSpell.words": [
      "Browsersync",
      "CSEE",
      "Hackathon",
      "Henrique",
      "Microservices",
      "Phan",
      "Tailwindcss",
      "createapp",
      "esbenp",
      "etajs",
      "fullhash",
      "happypack",
      "jsxdev",
      "lcov",
      "prettierrc"
   ],
   "cSpell.ignorePaths": [
      "**/package-lock.json",
      "**/node_modules/**",
      "**/vscode-extension/**",
      "**/.git/objects/**",
      ".vscode",
      ".vscode-insiders",
      ".env"
   ],
   "editor.someSetting": true,
   "markdownlint.config": {
      "default": true,
      "MD041": false,
      "MD030": false,
      "MD007": { "indent": 3 },
      "no-hard-tabs": false
   },
   "editor.codeActionsOnSave": {
      "source.fixAll.markdownlint": true
   },
   "prettier.tabWidth": 3,
   "prettier.trailingComma": "all",
   "prettier.singleQuote": true,
   "files.eol": "\n",
   "editor.formatOnType": true,
   "html.format.wrapLineLength": 80
}
