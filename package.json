{
   "name": "nodejs-starters",
   "version": "0.1.0",
   "description": "A collection of Node.js starters to get started with a modern web app development",
   "main": "app.ts",
   "scripts": {
      "clean": "rm -rf dist/** reports/** logs/**",
      "eslint": "npx eslint '**/*.{js,jsx,ts,tsx}'",
      "eslint:fix": "npx eslint '**/*.{js,jsx,ts,tsx}' --fix",
      "test:jest": "npx jest --forceExit --coverage --verbose",
      "watch:jest": "npx jest --forceExit --coverage --verbose --watch",
      "build:tsc": "npx tsc",
      "watch:ts": "npx nodemon --config src/config/nodemon.json --inspect src/server.ts",
      "watch:js": "npm run build:tsc && npx nodemon --config src/config/nodemon.json --inspect dist/server.js",
      "server:ts": "npx ts-node src/server.ts",
      "server:js": "npm run build:tsc && node dist/server.js"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/votungphan/nodejs-starters.git"
   },
   "keywords": [
      "NodeJS",
      "Node.js",
      "ExpressJS",
      "Express.js",
      "TypeScript",
      "JavaScript",
      "Jest",
      "SuperTest",
      "Bootstrap",
      "Eta",
      "ESLint",
      "Babel",
      "Prettier",
      "Webapp",
      "Web-app",
      "Web app"
   ],
   "author": "Phan Vo <votungphan@gmail.com>",
   "license": "MIT",
   "bugs": {
      "url": "https://github.com/votungphan/nodejs-starters/issues"
   },
   "homepage": "https://github.com/votungphan/nodejs-starters#readme",
   "devDependencies": {
      "@babel/core": "^7.13.14",
      "@babel/eslint-parser": "^7.13.14",
      "@babel/eslint-plugin": "^7.13.10",
      "@babel/plugin-proposal-class-properties": "^7.13.0",
      "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
      "@babel/preset-env": "^7.13.12",
      "@types/browser-sync": "^2.26.1",
      "@types/jest": "^26.0.22",
      "@types/node": "^14.14.37",
      "@types/supertest": "^2.0.11",
      "@typescript-eslint/eslint-plugin": "^4.20.0",
      "@typescript-eslint/parser": "^4.20.0",
      "babel-preset-airbnb": "^5.0.0",
      "browser-sync": "^2.26.14",
      "eslint": "^7.23.0",
      "eslint-config-airbnb-base": "^14.2.1",
      "eslint-config-airbnb-typescript": "^12.3.1",
      "eslint-config-prettier": "^8.1.0",
      "eslint-plugin-import": "^2.22.1",
      "eslint-plugin-prettier": "^3.3.1",
      "jest": "^26.6.3",
      "jest-html-reporters": "^2.1.2",
      "jest-junit": "^12.0.0",
      "jest-stare": "^2.2.1",
      "nodemon": "^2.0.7",
      "prettier": "^2.2.1",
      "supertest": "^6.1.3",
      "ts-jest": "^26.5.4",
      "ts-node": "^9.1.1",
      "typescript": "^4.2.3"
   },
   "dependencies": {
      "@types/express": "^4.17.11",
      "@types/pino-multi-stream": "^5.1.0",
      "eta": "^1.12.1",
      "express": "^4.17.1",
      "pino-multi-stream": "^5.3.0",
      "pino-pretty": "^4.7.1"
   }
}
