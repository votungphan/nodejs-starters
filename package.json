{
   "name": "nodejs-starters",
   "version": "0.1.0",
   "description": "A collection of Node.js starters to get started with a modern web app development",
   "main": "app.ts",
   "scripts": {
      "clean": "rm -rf dist/**",
      "test": "jest",
      "build-tsc": "npx tsc",
      "watch-ts": "npx nodemon --config src/config/nodemon.json src/server.ts",
      "watch-ts:debug": "npx nodemon --config src/config/nodemon.json --inspect src/server.ts",
      "server-ts": "npx ts-node src/server.ts",
      "server-js": "npm run build-tsc && node dist/server.js"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/votungphan/nodejs-starters.git"
   },
   "keywords": [
      "NodeJS",
      "Node.js",
      "ExpressJS",
      "Express.js",
      "TypeScript",
      "JavaScript",
      "Jest",
      "SuperTest",
      "Bootstrap",
      "Eta",
      "ESLint",
      "Babel",
      "Prettier",
      "Webapp",
      "Web-app",
      "Web app"
   ],
   "author": "Phan Vo <votungphan@gmail.com>",
   "license": "MIT",
   "bugs": {
      "url": "https://github.com/votungphan/nodejs-starters/issues"
   },
   "homepage": "https://github.com/votungphan/nodejs-starters#readme",
   "devDependencies": {
      "@babel/core": "^7.13.14",
      "@babel/eslint-parser": "^7.13.14",
      "@babel/eslint-plugin": "^7.13.10",
      "@babel/plugin-proposal-class-properties": "^7.13.0",
      "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
      "@babel/preset-env": "^7.13.12",
      "@babel/preset-typescript": "^7.13.0",
      "@types/browser-sync": "^2.26.1",
      "@types/express": "^4.17.11",
      "@types/node": "^14.14.37",
      "@typescript-eslint/eslint-plugin": "^4.20.0",
      "@typescript-eslint/parser": "^4.20.0",
      "babel-preset-airbnb": "^5.0.0",
      "browser-sync": "^2.26.14",
      "eslint": "^7.23.0",
      "eslint-config-airbnb-base": "^14.2.1",
      "eslint-config-airbnb-typescript": "^12.3.1",
      "eslint-config-prettier": "^8.1.0",
      "eslint-multiple-parsers": "^1.0.1",
      "eslint-plugin-import": "^2.22.1",
      "eslint-plugin-prettier": "^3.3.1",
      "express": "^4.17.1",
      "nodemon": "^2.0.7",
      "prettier": "^2.2.1",
      "ts-node": "^9.1.1",
      "typescript": "^4.2.3"
   }
}
